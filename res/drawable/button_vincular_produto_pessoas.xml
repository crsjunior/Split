<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >

	<item android:state_pressed="true">
		<shape android:shape="oval">

			<solid
				android:color="#ffabc8d6" />

			<stroke
				android:width="1dp"
				android:color="#ffabc8d6"/>

			<padding
				android:bottom="0dp"
				android:left="0dp"
				android:right="0dp"
				android:top="0dp"/>
		</shape>
	</item>

	<item>
		<shape android:shape="oval">

			<solid
				android:color="#ffc0e0f0" />

			<stroke
				android:width="1dp"
				android:color="#ffabc8d6"/>

			<padding
				android:bottom="0dp"
				android:left="0dp"
				android:right="0dp"
				android:top="0dp"/>
		</shape>
	</item>

</selector>
